@article{hanMetabolomicsPipelineMechanistic2021,
  title = {A Metabolomics Pipeline for the Mechanistic Interrogation of the Gut Microbiome},
  author = {Han, Shuo and Van Treuren, Will and Fischer, Curt R. and Merrill, Bryan D. and DeFelice, Brian C. and Sanchez, Juan M. and Higginbottom, Steven K. and Guthrie, Leah and Fall, Lalla A. and Dodd, Dylan and Fischbach, Michael A. and Sonnenburg, Justin L.},
