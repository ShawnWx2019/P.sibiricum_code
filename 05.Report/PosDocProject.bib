@article{chenAntimicrobialEvaluationMyricetin2021,
  title = {Antimicrobial Evaluation of Myricetin Derivatives Containing Benzimidazole Skeleton against Plant Pathogens},
  author = {Chen, Mei and Tang, Xuemei and Liu, Tingting and Peng, Feng and Zhou, Qing and Luo, Hui and He, Ming and Xue, Wei},
  year = {2021},
