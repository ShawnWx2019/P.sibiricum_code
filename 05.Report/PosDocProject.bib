  pmcid = {PMC6850026},
  pmid = {30537259},
  keywords = {/unread,AGPv4,B73,Chromosome Mapping,co-expression,differential expression,gene atlas,Gene Expression Profiling,Genome; Plant,Genomics,Genotype,Hybrid Vigor,Organ Specificity,Phenotype,presence−absence variants,Stress; Physiological,Transcriptome,Zea mays},
