}

@article{ibdahIdentificationCloningNADPHdependent2014,
  title = {Identification and Cloning of an {{NADPH-dependent}} Hydroxycinnamoyl-{{CoA}} Double Bond Reductase Involved in Dihydrochalcone Formation in {{Malus}}\texttimes domestica {{Borkh}}.},
  author = {Ibdah, Mwafaq and Berim, Anna and Martens, Stefan and Valderrama, Andrea Lorena Herrera and Palmieri, Luisa and Lewinsohn, Efraim and Gang, David R.},
