}

@article{luoTranscriptomewideAnalysisEpitranscriptome2021,
  title = {Transcriptome-Wide Analysis of Epitranscriptome and Translational Efficiency Associated with Heterosis in Maize},
  author = {Luo, Jin-Hong and Wang, Min and Jia, Gui-Fang and He, Yan},
  year = {2021},
