@article{luoOptimizationLargescalePseudotargeted2016,
  title = {Optimization of Large-Scale Pseudotargeted Metabolomics Method Based on Liquid Chromatography\textendash Mass Spectrometry},
  author = {Luo, Ping and Yin, Peiyuan and Zhang, Weijian and Zhou, Lina and Lu, Xin and Lin, Xiaohui and Xu, Guowang},
  year = {2016},
  month = mar,
  journal = {Journal of Chromatography A},
  volume = {1437},
