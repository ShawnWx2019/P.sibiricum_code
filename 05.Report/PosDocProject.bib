@article{hishinumaWideTargetedMetabolomeAnalysis2021,
  title = {Wide-{{Targeted Metabolome Analysis Identifies Potential Biomarkers}} for {{Prognosis Prediction}} of {{Epithelial Ovarian Cancer}}},
  author = {Hishinuma, Eiji and Shimada, Muneaki and Matsukawa, Naomi and Saigusa, Daisuke and Li, Bin and Kudo, Kei and Tsuji, Keita and Shigeta, Shogo and Tokunaga, Hideki and Kumada, Kazuki and Komine, Keigo and Shirota, Hidekazu and Aoki, Yuichi and Motoike, Ikuko N. and Yasuda, Jun and Kinoshita, Kengo and Yamamoto, Masayuki and Koshiba, Seizo and Yaegashi, Nobuo},
