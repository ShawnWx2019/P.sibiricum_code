@article{liTemporalRegulationMetabolome2020,
  title = {Temporal {{Regulation}} of the {{Metabolome}} and {{Proteome}} in {{Photosynthetic}} and {{Photorespiratory Pathways Contributes}} to {{Maize Heterosis}}},
  author = {Li, Zhi and Zhu, Andan and Song, Qingxin and Chen, Helen Y. and Harmon, Frank G. and Chen, Z. Jeffrey},
