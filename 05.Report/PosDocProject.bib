@article{huTranscriptomeProfilingComparison2016a,
  title = {Transcriptome Profiling and Comparison of Maize Ear Heterosis during the Spikelet and Floret Differentiation Stages},
  author = {Hu, Xiaojiao and Wang, Hongwu and Diao, Xizhou and Liu, Zhifang and Li, Kun and Wu, Yujin and Liang, Qianjin and Wang, Hui and Huang, Changling},
